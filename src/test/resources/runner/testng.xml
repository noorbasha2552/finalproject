<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="DataDriven">
<listeners>
      <listener class-name="org.uncommons.reportng.HTMLReporter"/>
      <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
      <listener class-name="com.w2a.listeners.CustomListeners"/>
  </listeners>
  
  
  <test thread-count="5" name="logintest">
    <classes>
    	<class name="com.w2a.testcases.LoginTest"/>
        </classes>   
  </test>
  
  <test thread-count="5" name="Adding customer ">
    <classes>
    	  	<class name="com.w2a.testcases.AddCust"/>
    </classes>
  </test> 
  
  <test thread-count="5" name="opening account">
    <classes>
    	  	<class name="com.w2a.testcases.OpenAccount"/>
    </classes>
  </test>
  
  
</suite> <!-- Suite -->
